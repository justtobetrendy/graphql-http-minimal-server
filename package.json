{
  "name": "graphql-http-minimal-server",
  "version": "1.0.0",
  "description": "Example of a minimal GraphQL server",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "npx tsc ",
    "prestart": "npm run codegen",
    "start": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justtobetrendy/graphql-http-minimal-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justtobetrendy/graphql-http-minimal-server/issues"
  },
  "homepage": "https://github.com/justtobetrendy/graphql-http-minimal-server#readme",
  "dependencies": {
    "@graphql-tools/schema": "^9.0.17",
    "dataloader": "^2.2.2",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-http": "^1.17.1",
    "graphql-tag": "^2.12.6",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/introspection": "3.0.1",
    "@graphql-codegen/typescript": "3.0.2",
    "@graphql-codegen/typescript-resolvers": "3.1.1",
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.3"
  }
}
